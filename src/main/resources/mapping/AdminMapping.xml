<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.dlufl.ohrigei.dao.AdminDao">
    <select id="queryAllDelegate" resultMap="User">
        select ohrigei.user.name, ohrigei.user.email
        from ohrigei.user,
             ohrigei.delegate
        where delegate.role_ID = 1
          and delegate.id = user.id
    </select>
    <select id="queryAllObserver" resultMap="Delegate">
    </select>
    <select id="queryAllLeader" resultMap="Delegate">
    </select>
    <select id="queryAllTeacher" resultMap="Delegate">
    </select>
    <select id="queryAllGroup" resultMap="Group">
    </select>
    <select id="queryAllAdmin" resultMap="Admin">
    </select>
    <select id="queryAllCommittee" resultMap="Committee">
    </select>
</mapper>